- if user_signed_in?
  %h6 Your Gists
  .gists
    %ul.unstyled
      - current_user.github_gists.each do |gist|
        %li= link_to "##{gist['id']} #{gist["description"]}", "#import_gist_#{gist['id']}", :class => "import_gist", :id => gist["id"]

%h6 Tags
%ul.unstyled
  - Tag.all.each do |t|
    %li= link_to t, tags_path(t)